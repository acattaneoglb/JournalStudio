<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <solid android:color="#ffffded4"/>
            <stroke android:width="2dp" android:color="#ff8c7c78"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <solid android:color="#ffcc6741"/>
            <stroke android:width="2dp" android:color="#ff8b3e2a"/>
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
            <solid android:color="#ffff7f51"/>
            <stroke android:width="2dp" android:color="#ff59291d"/>
        </shape>
    </item>
</selector>