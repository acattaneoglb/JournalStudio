<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <solid android:color="#ffdbe6b5"/>
            <stroke android:width="2dp" android:color="#ffa9b38d"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <solid android:color="#ffa2c621"/>
            <stroke android:width="2dp" android:color="#ff627916"/>
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
            <solid android:color="#ffbde927"/>
            <stroke android:width="2dp" android:color="#ff42510a"/>
        </shape>
    </item>
</selector>