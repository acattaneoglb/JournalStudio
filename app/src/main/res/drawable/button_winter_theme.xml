<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <solid android:color="#ffd3e9f0"/>
            <stroke android:width="2dp" android:color="#ff6e7678"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <solid android:color="#ff6cbad0"/>
            <stroke android:width="2dp" android:color="#ff4b8496"/>
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
            <solid android:color="#ff7dd7f1"/>
            <stroke android:width="2dp" android:color="#ff2e525d"/>
        </shape>
    </item>
</selector>